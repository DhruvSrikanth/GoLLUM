func main() {
    var a int;
    var b int;
    b = 5;
    a = 1;
    for (a < 10) {
        a = a + 1;
        if (a == b) {
            printf("a == b");
        }
        if (a != b) {
            printf("a != b");
        }
        if (a < b) {
            printf("a < b");
        }
        if (a > b) {
            printf("a > b");
        }
        if (a <= b) {
            printf("a <= b");
        }
        if (a >= b) {
            printf("a >= b");
        }
    }
}