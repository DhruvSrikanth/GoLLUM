func fact (x int) int  {

	if(x <= 1) {
		return 1;
	} else {
		return (x * fact(x-1));
	}
}

func main () {
	var stop bool;
	var factor, toStop, temp int;

	stop = false;
	factor = 0;
	
	for(!stop) {
		scan factor;
        temp = fact(factor); 
    	printf("%d",temp); 
        scan toStop;
		if(toStop == 0) {
			stop = true;
		}
	}
}